{% extends "EherQueroSerVoluntarioFrontendBundle::layout.html.twig" %}
{% block title %}{% trans %}Quero ser Voluntário{% endtrans %} - {% trans %}Parabéns pelo Cadastro{% endtrans %}{% endblock %}
{% block content %}
    <h2>{% trans %}Parabéns pelo Cadastro{% endtrans %}</h2>
    <p>{% trans %}Assim que encontrarmos uma oportunidade relacionada ao perfil cadastrado, entraremos em contato{% endtrans %}.</p>
    <p>{% trans %}Aproveite para{% endtrans %}:</p>
    <ul>
        <li><a href="{{ path('voluntario_cadastrar') }}">{% trans %}Cadastrar outra pessoa{% endtrans %}</a></li>
        <li><a href="{{ path('entidade_cadastrar') }}">{% trans %}Cadastrar uma entidade{% endtrans %}</a></li>
    </ul>
{% endblock %}
